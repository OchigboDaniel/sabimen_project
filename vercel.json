{
    "version": 2,
    "builds": [
        {
            "source": "./index.js", 
            "use": "@vercel/node" }
    ],

    "routes": [
        {
            "source": "/(.*)", 
            "udestination": "/" }
    ],

    "rewrites": [
        {
            "source": "/api/(.*)", 
            "udestination": "/api" }
        ]


}